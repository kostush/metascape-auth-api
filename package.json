{
  "name": "metascape-auth-api",
  "version": "0.0.1",
  "description": "",
  "author": "Vodworks",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,migration}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test,migration}/**/*.ts\" --fix",
    "proto:win": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd -I=./resources/proto --ts_proto_opt=nestJs=true --ts_proto_out=src/auth resources/proto/auth.proto --ts_proto_opt=fileSuffix=.pb",
    "proto:mac": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto -I=./resources/proto --ts_proto_opt=nestJs=true --ts_proto_out=src/auth resources/proto/auth.proto --ts_proto_opt=fileSuffix=.pb",
    "test": "jest --config ./test/jest.config.ts --runInBand",
    "test:watch": "jest --config ./test/jest.config.ts --runInBand --watch",
    "test:cov": "jest --config ./test/jest.config.ts --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "@grpc/proto-loader": "^0.7.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.0.8",
    "@nestjs/platform-express": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "metascape-common-api": "git+ssh://git@bitbucket.org:proj-metascape/metascape-common-api#0.2.1",
    "metascape-user-api-client": "bitbucket:proj-metascape/metascape-user-api-client#0.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.122.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.13.2",
    "npm": ">=8.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:proj-metascape/auth-api.git"
  }
}
